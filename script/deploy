# Temporarily store uncommited changes
#git stash

# Verify correct branch
git checkout dev

# Build new files
stack exec rbio clean
stack exec rbio build

# Get previous files
git fetch --all
git checkout master
git rebase origin/master

# Overwrite existing files with new files
cp -a _site/. .

# Commit
git add -A
git commit -m "publish"

# Push
git push

# Restoration
git checkout dev
#git stash pop
